<?xml version="1.0" encoding="UTF-8"?>	
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.9.0//EN"
    "https://gwtproject.org/doctype/2.9.0/gwt-module.dtd">
<module rename-to='gwtmdc'>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.i18n.I18N"/>

  <!-- Specify the app entry point class.                         -->
  <entry-point class='gwt.material.design.components.client.GwtMDC'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  
  <!-- public resources -->
  <public path="public" />

  <!-- Load the Material Design Icons -->
  <stylesheet src="https://fonts.googleapis.com/icon?family=Material+Icons"/>
  <!-- 
  <stylesheet src="https://fonts.googleapis.com/css?family=Roboto:300,400,500"/> 
  <stylesheet src="https://fonts.googleapis.com/css?family=Roboto+Mono"/>      	
  <stylesheet src="https://fonts.googleapis.com/css?family=Google+Sans"/>
   -->
  <stylesheet src="css/product_sans.css"/> 
  <!-- allow Super Dev Mode -->
  <!-- <add-linker name="xsiframe"/> -->  
  
  <!-- Supported languages -->
  <extend-property name="locale" values="en"/>
  <extend-property name="locale" values="pt_BR" /> 
  
  <!-- Default language --> 
  <set-property-fallback name="locale" value="en"/>
      
  <!-- Google Material Components Web v0.41.0 -->
  
</module>
